{
  "match": {
    "${$validator}": "/@?dcic/signature-commons-schema/(v[0-9]+/)?core/library.json",
    "${dataset_type}": "geneset_library"
  },
  "properties": {
    "Image": {
      "visibility": 1,
      "type": "img",
      "src": "${PREFIX}/${meta.creators[0].affiliations[0].extraProperties[0].values[0]}",
      "alt": "${meta['title']}",
      "text": "${meta['title']}",
      "priority": 1,
      "icon": true,
      "name": true
    },
    "Description": {
      "visibility": 1,
      "type": "text",
      "text": "${meta.description}",
      "priority": 1,
      "description": true,
      "subtitle": true
    },
    "Assay": {
      "visibility": 1,
      "type": "text",
      "text": "${meta.types[0].method.value}",
      "priority": 1,
      "MDI_Icon": "mdi-test-tube"
    },
    "Institute": {
      "visibility": 1,
      "type": "text",
      "text": "${meta.creators[0].affiliations[0].name}",
      "priority": 2,
      "MDI_Icon": "mdi-certificate"
    }
  }
}