{
  "match": {
    "${$validator}": "/@?dcic/signature-commons-schema/(v[0-9]+/)?core/resource.json"
  },
  "properties": {
    "Image": {
      "visibility": 1,
      "type": "img",
      "src": "${PREFIX}/${meta['Icon']}",
      "alt": "${meta['assay']}",
      "text": "${meta['assay']}",
      "priority": 1,
      "icon": true,
      "name": true
    },
    "Data Link": {
      "visibility": 1,
      "type": "text",
      "field": "data_link",
      "text": "${meta['data_link']}",
      "priority": 3
    },
    "Description": {
      "visibility": 1,
      "type": "text",
      "field": "description",
      "text": "${meta['description']}",
      "priority": 2,
      "description": true,
    },
    "Institution": {
      "visibility": 1,
      "type": "text",
      "field": "institution",
      "text": "${meta['institution']}",
      "priority": 3
    },
    "LINCS center": {
      "visibility": 1,
      "type": "text",
      "field": "LINCS_center",
      "text": "${meta['LINCS_center']}",
      "priority": 3
    },
    "Signature Count": {
      "visibility": 1,
      "type": "text",
      "field": "Signature_Count",
      "text": "${meta['Signature_Count']}",
      "priority": 3
    }
  }
}