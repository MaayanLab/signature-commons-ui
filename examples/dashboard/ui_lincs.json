[
  {
    "landing": true,
    "content": {
      "favicon": {
        "src": "/static/LINCS.ico",
        "alt": "LINCS Datasets and Signatures",
        "title": "LINCS Datasets and Signatures"
      },
      "header_info": {
          "icon": {
              "src": "/static/LINCS.ico",
              "alt": "lincs",
              "style": {
                "width": 60,
                "marginBottom": -15
              }
          },
          "header_right": " LINCS Datasets and Signatures"
      },
      "font_families": [
        "https://fonts.googleapis.com/css?family=Lato:100,300,400,700,900",
        "https://fonts.googleapis.com/css?family=Archivo+Narrow"
      ],
      "metadata_placeholder": "Search over LINCS datasets and signatures",
      "text_1": "Explore an extensive collection of LINCS datasets and signatures",
      "preferred_name_singular": {
        "libraries": "Dataset",
        "resources": "Data and Signature Generation Center"
      },
      "nav": {
        "MetadataSearch": {
          "active": true,
          "endpoint": "/MetadataSearch"
        },
        "SignatureSearch": {
          "active": true,
          "endpoint": "/SignatureSearch"
        },
        "Resources": {
          "active": true,
          "endpoint": "/DSGCs"
        },
        "API": {
          "active": true
        }
      },
      "footer_links": [
        {
          "href": "http://www.lincsproject.org/LINCS/data/overview",
          "src": "/static/NIHLINCS.jpg",
          "alt": "LINCS",
          "containerProps": {
            "style": {
              "width": 300
            }
          },
          "imageProps": {
            "style": {
              "width": 300
            }
          }
        },
        {
          "href": "http://lincs-dcic.org/",
          "src": "/static/images/dcic_light.png",
          "alt": "dcic"
        }
      ],
      "theme_mod": {
        "palette": {
          "primary": {
            "main": "rgb(100,150,200)"
          },
          "secondary": {
            "main": "rgb(204, 204, 153)"
          },
          "default": {
            "main": "#555",
            "contrastText": "#FFF"
          }
        },
        "typography": {
          "fontFamily": "Lato, serif, Archivo Narrow"
        }
      },
      "deactivate_download": false,
      "bar_chart": {
        "Field_Name": "types.method.value",
        "Caption": "Assays"
      },
      "bar_chart_style": {
        "ResponsiveContainer": {
          "height":420,
          "width": 420
        },
        "Chart": {
          "margin": {
            "right": 80,
            "left": 50
          }
        },
        "XAxis": {
          "axisLine": false,
          "angle": 45,
          "height": 180,
          "textAnchor": "start",
          "interval": 0,
          "tick": {
            "fontSize": 9
          }
        },
        "Bar": {
          "fill": "rgb(124, 162, 206)"
        }
      },
      "pie_chart_style": {
        "Pie": {
          "fill":  "rgb(124, 162, 206)"
        }
      },
      "counting_validator": "/dcic/signature-commons-schema/v5/meta/schema/counting.json",
      "ui_schema": "/dcic/signature-commons-schema/v5/meta/schema/ui-schema.json",
      "search_terms": [
        "Apoptosis",
        "Interferon",
        "Autophagy",
        "PKN3"
      ]
    }
  },
  {
    "admin": true,
    "content": {
      "preferred_name_singular": {
        "libraries": "Dataset",
        "resources": "Dataset"
      },
      "bar_chart": {
        "Field_Name": "types.method.value",
        "Caption": "Assays"
      },
      "bar_chart_style": {
        "ResponsiveContainer": {
          "width": "100%",
          "height":500
        },
        "Chart": {
          "margin": {
            "right": 50,
            "left": 50
          }
        },
        "XAxis": {
          "axisLine": false,
          "angle": 45,
          "height": 180,
          "textAnchor": "start",
          "interval": 0,
          "tick": {
            "fontSize": 9
          }
        },
        "Bar": {
          "fill": "rgb(124, 162, 206)"
        }
      },
      "resource_name_from_library": "title",
      "resource_name": "${meta.name}",
      "resource_icon": "${meta.extraProperties[0].values[0]}",
      "library_name": "title",
      "counting_validator": "/dcic/signature-commons-schema/v5/meta/schema/counting.json"
    }
  }
]