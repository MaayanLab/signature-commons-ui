  "scripts": {
    "test": "npm run lint && jest",
    "lint": "eslint next.config.js util/** pages/** styles/jss/** components/**",
    "lint:fix": "npm run lint -- --fix",
    "dev": "NODE_ENV=development dotenv-load next",
    "build": "NODE_ENV=production dotenv-load next build",
    "build:docker": "dotenv-load node ./bin/docker-build.js",
